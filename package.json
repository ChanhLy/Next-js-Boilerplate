{
  "name": "next-js-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "ci": "rm -rf node_modules && yarn install --frozen-lockfile",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "export": "next export",
    "build-prod": "run-s clean build export",
    "clean": "rimraf .next out",
    "lint": "next lint",
    "build-types": "tsc --noEmit --pretty",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test:watch": "jest --watch --json --outputFile=.jest-test-results.json",
    "test": "jest",
    "test:ci": "jest --ci",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test start http://localhost:3000 cypress:headless",
    "prepare": "husky install",
    "storybook": "start-storybook -s public -p 6006",
    "prebuild:storybook": "yarn test:generate-output",
    "build:storybook": "build-storybook -s public",
    "chromatic": "npx chromatic --exit-zero-on-changes buildScriptName=build:storybook"
  },
  "dependencies": {
    "@chanhly/cui": "https://github.com/ChanhLySolutions/cui",
    "@sentry/nextjs": "^6.13.3",
    "http-status-codes": "^2.1.4",
    "next": "^12.0.5",
    "next-auth": "^4.0.0-beta.4",
    "next-seo": "^4.28.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-jsx-plugin-postcss": "^4.0.1",
    "webpack": "5"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@next/bundle-analyzer": "^12.0.0",
    "@next/eslint-plugin-next": "^12.0.0",
    "@storybook/addon-actions": "^6.4.0",
    "@storybook/addon-essentials": "^6.4.0",
    "@storybook/addon-jest": "^6.4.0",
    "@storybook/addon-links": "^6.4.0",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.4.0",
    "@storybook/manager-webpack5": "^6.4.0",
    "@storybook/react": "^6.4.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "autoprefixer": "^10.3.7",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "chromatic": "^6.0.4",
    "cross-env": "^7.0.3",
    "cypress": "^8.7.0",
    "eslint": "<8.0.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-next": "^12.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.14.0",
    "storybook-addon-next-router": "^3.0.8",
    "tailwindcss": "^2.2.17",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.4.4"
  },
  "license": "ISC"
}
